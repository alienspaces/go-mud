{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://gitlab.com/alienspaces/go-mud/backend/schema/docs/dungeoninstance/data.schema.json",
  "title": "Dungeon Instance Data",
  "description": "Dungeon instance data",
  "type": "object",
  "required": ["dungeon_id","dungeon_instance_id",  "dungeon_instance_name", "dungeon_instance_description", "dungeon_instance_created_at"],
  "properties": {
    "dungeon_id": {
      "type": "string",
      "format": "uuid"
    },
    "dungeon_instance_id": {
      "type": "string",
      "format": "uuid"
    },
    "dungeon_instance_name": {
      "type": "string"
    },
    "dungeon_instance_description": {
      "type": "string"
    },
    "dungeon_instance_created_at": {
      "type": "string",
      "format": "date-time"
    },
    "dungeon_instance_updated_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}
