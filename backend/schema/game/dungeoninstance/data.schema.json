{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://gitlab.com/alienspaces/go-mud/backend/schema/game/dungeoninstance/data.schema.json",
  "title": "Dungeon Instance Data",
  "description": "Dungeon instance data",
  "type": "object",
  "required": ["id", "dungeon_id", "name", "description", "created_at"],
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "dungeon_id": {
      "type": "string",
      "format": "uuid"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "created_at": {
      "type": "string",
      "format": "date-time"
    },
    "updated_at": {
      "type": "string",
      "format": "date-time"
    }
  }
}
