{
  "version": 2,
  "extra_config": {
    "github_com/devopsfaith/krakend-cors": {
      "allow_origins": ["*"],
      "expose_headers": ["Content-Length", "Authorization"],
      "max_age": "12h",
      "allow_methods": ["GET", "HEAD", "POST", "PUT", "DELETE", "OPTIONS"],
      "allow_credentials": false,
      "allow_headers": ["Authorization", "Content-Type"]
    },
    "github_com/devopsfaith/krakend-gologging": {
      "level": "DEBUG",
      "prefix": "[KRAKEND]",
      "syslog": false,
      "stdout": true,
      "format": "default"
    },
    "github_com/devopsfaith/krakend-logstash": {
      "enabled": false
    }
  },
  "timeout": "3000ms",
  "cache_ttl": "300s",
  "output_encoding": "json",
  "name": "go-mud",
  "endpoints": [
    {
      "endpoint": "/api",
      "method": "GET",
      "output_encoding": "no-op",
      "extra_config": {},
      "backend": [
        {
          "url_pattern": "/api",
          "encoding": "no-op",
          "sd": "static",
          "method": "GET",
          "host": ["http://172.17.0.1:8084"],
          "disable_host_sanitize": true
        }
      ]
    },
    {
      "endpoint": "/api/dungeons",
      "headers_to_pass": ["Authorization"],
      "method": "GET",
      "output_encoding": "no-op",
      "extra_config": {},
      "querystring_params": [],
      "backend": [
        {
          "url_pattern": "/api/dungeons",
          "encoding": "no-op",
          "method": "GET",
          "host": ["http://172.17.0.1:8084"],
          "disable_host_sanitize": true
        }
      ]
    },
    {
      "endpoint": "/api/dungeons/{dungeon_id}",
      "headers_to_pass": ["Authorization"],
      "method": "GET",
      "output_encoding": "no-op",
      "extra_config": {},
      "backend": [
        {
          "url_pattern": "/api/dungeons/{dungeon_id}",
          "encoding": "no-op",
          "method": "GET",
          "host": ["http://172.17.0.1:8084"],
          "disable_host_sanitize": true
        }
      ]
    }
  ]
}
